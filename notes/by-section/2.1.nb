Cell[TextData[{
 StyleBox["2",
  FontWeight->"Bold"],
 " Define ",
 StyleBox["path integral equation",
  FontSlant->"Italic"],
 " (PIE) (or ",
 StyleBox["operator equation",
  FontSlant->"Italic"],
 " in Polchinski\[CloseCurlyQuote]s words) as follow. For a general \
configuration ",
 Cell[BoxData[
  FormBox[
   RowBox[{"X", "(", "\[Sigma]", ")"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", and for a general functional (operator) ",
 Cell[BoxData[
  FormBox["F", TraditionalForm]],
  FormatType->"TraditionalForm"],
 " of ",
 Cell[BoxData[
  FormBox["X", TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", path integral equation ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"F", "[", "X", "]"}], "=", "0"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " holds iff"
}], "Text",
 CellChangeTimes->{{3.659943999599756*^9, 3.659944016774899*^9}, {
  3.659944916129864*^9, 3.659944919356344*^9}, {3.659945056016952*^9, 
  3.659945071515197*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"\[Integral]", 
     RowBox[{
      RowBox[{"\[ScriptCapitalD]", "[", "X", "]"}], " ", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         SubscriptBox["W", 
          RowBox[{"I", "/", "O"}]], "[", "X", "]"}], " ", 
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{"-", 
          RowBox[{"S", "[", "X", "]"}]}]]}], ")"}], "\[Times]", 
      RowBox[{"F", "[", "X", "]"}]}]}], "=", "0"}], " ", ","}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.659944020461763*^9, 3.659944106085271*^9}, {
  3.659944149323538*^9, 3.65994415031561*^9}}],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["S", TraditionalForm]],
  FormatType->"TraditionalForm"],
 " is the action of the physical system (Euclidean at here, but can be \
extended to general), and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["W", 
     RowBox[{"I", "/", "O"}]], "[", "X", "]"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " the weight functional which represents the input and output states. So, \
the ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["W", 
    RowBox[{"I", "/", "O"}]], TraditionalForm]]],
 " depends only on ",
 Cell[BoxData[
  FormBox[
   RowBox[{"X", "(", 
    RowBox[{
     SuperscriptBox["\[Sigma]", "1"], ",", 
     RowBox[{
      SuperscriptBox["\[Sigma]", "2"], "=", 
      RowBox[{"-", "\[Infinity]"}]}]}], ")"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " (input) and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"X", "(", 
    RowBox[{
     SuperscriptBox["\[Sigma]", "1"], ",", 
     RowBox[{
      SuperscriptBox["\[Sigma]", "2"], "=", 
      RowBox[{"+", "\[Infinity]"}]}]}], ")"}], TraditionalForm]]],
 " (output), rather than any ",
 Cell[BoxData[
  FormBox[
   RowBox[{"X", "(", 
    RowBox[{
     SuperscriptBox["\[Sigma]", "1"], ",", 
     SuperscriptBox["\[Sigma]", "2"]}], ")"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " with ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[Sigma]", "2"], "\[NotEqual]", 
    RowBox[{"\[PlusMinus]", "\[Infinity]"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "."
}], "Text",
