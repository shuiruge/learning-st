 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"T", "(", "z", ")"}], ":=", 
    RowBox[{
     SubscriptBox["T", "zz"], "(", "z", ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ")"
}], "Text",
 CellChangeTimes->{{3.673512761117737*^9, 3.673512804490773*^9}, {
  3.67351302400618*^9, 3.673513030866865*^9}, {3.673513452270168*^9, 
  3.673513471244694*^9}}],

Cell[BoxData[
 FormBox[GridBox[{
    {
     RowBox[{
      RowBox[{
       SuperscriptBox["T", "\[Prime]"], "(", 
       RowBox[{
        SuperscriptBox["z", "\[Prime]"], "(", "z", ")"}], ")"}], 
      "\[Congruent]", 
      RowBox[{
       SubscriptBox[
        SuperscriptBox["T", "\[Prime]"], "zz"], "(", 
       RowBox[{
        SuperscriptBox["z", "\[Prime]"], "(", "z", ")"}], ")"}]}], "=", 
     RowBox[{
      FractionBox[
       RowBox[{"\[PartialD]", "z"}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["z", "\[Prime]"]}]], " ", 
      FractionBox[
       RowBox[{"\[PartialD]", "z"}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["z", "\[Prime]"]}]], " ", 
      RowBox[{
       SubscriptBox["T", "zz"], "(", "z", ")"}]}]},
    {"\[Placeholder]", "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "-", 
        RowBox[{"\[Epsilon]", " ", 
         RowBox[{"\[PartialD]", 
          RowBox[{"v", "(", "z", ")"}]}]}]}], ")"}], " ", 
      RowBox[{"(", 
       RowBox[{"1", "-", 
        RowBox[{"\[Epsilon]", " ", 
         RowBox[{"\[PartialD]", 
          RowBox[{"v", "(", "z", ")"}]}]}]}], ")"}], " ", 
      RowBox[{
       SubscriptBox["T", "zz"], "(", "z", ")"}]}]},
    {"\[Placeholder]", "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "-", 
        RowBox[{"2", " ", "\[Epsilon]", " ", 
         RowBox[{"\[PartialD]", 
          RowBox[{"v", "(", "z", ")"}]}]}]}], ")"}], " ", 
      RowBox[{
       SubscriptBox["T", "zz"], "(", 
       RowBox[{
        RowBox[{
         SuperscriptBox["z", "\[Prime]"], "(", "z", ")"}], "-", 
        RowBox[{"\[Epsilon]", " ", 
         RowBox[{"v", "(", "z", ")"}]}]}], ")"}]}]},
    {"\[Placeholder]", "=", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1", "-", 
         RowBox[{"2", " ", "\[Epsilon]", " ", 
          RowBox[{"\[PartialD]", "v"}], 
          RowBox[{"(", 
           SuperscriptBox["z", "\[Prime]"], ")"}]}]}], ")"}], " ", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{
          SubscriptBox["T", "zz"], "(", 
          SuperscriptBox["z", "\[Prime]"], ")"}], "-", 
         RowBox[{"\[Epsilon]", " ", 
          RowBox[{"v", "(", 
           SuperscriptBox["z", "\[Prime]"], ")"}], " ", 
          RowBox[{"\[PartialD]", 
           RowBox[{
            SubscriptBox["T", "zz"], "(", 
            SuperscriptBox["z", "\[Prime]"], ")"}]}]}]}], "}"}]}], " ", 
      ","}]}
   }], TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.673513032505804*^9, 3.673513359228746*^9}, {
