Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{
     SubscriptBox["\[ContourIntegral]", 
      SubscriptBox["C", "2"]], 
     RowBox[{
      RowBox[{"\[DifferentialD]", 
       SubscriptBox["z", "2"]}], " ", 
      RowBox[{
       SubscriptBox["\[ContourIntegral]", 
        RowBox[{"\[CapitalDelta]C", "(", 
         SubscriptBox["z", "2"], ")"}]], 
       RowBox[{
        RowBox[{"\[DifferentialD]", "z"}], " ", 
        RowBox[{
         SubscriptBox["j", "1"], "(", "z", ")"}], " ", 
        RowBox[{
         SubscriptBox["j", "2"], "(", 
         SubscriptBox["z", "2"], ")"}]}]}]}]}], "=", 
    RowBox[{
     SubscriptBox["\[ContourIntegral]", 
      SubscriptBox["C", "2"]], 
     RowBox[{
      RowBox[{"\[DifferentialD]", 
       SubscriptBox["z", "2"]}], " ", 
      UnderscriptBox["Res", 
